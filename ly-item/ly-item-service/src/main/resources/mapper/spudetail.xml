<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.leyou.item.mapper.SpuDetailMapper">

    <insert id="insertSpuDetail">
        INSERT INTO tb_spu_detail
            (`spu_id`, `description`, `specifications`, `spec_template`, `packing_list`, `after_service`)
        VALUES
            (#{spuId}, #{spuDetail.description}, #{spuDetail.specifications}, #{spuDetail.specTemplate}, #{spuDetail.packingList}, #{spuDetail.afterService})
    </insert>




</mapper>